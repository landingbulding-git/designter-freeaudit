{
  "version": 2,
  "builds": [
    {
      "src": "apps/main-website/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "apps/landing-pages/landing-ingyenes-audit/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/ingyenes-audit/:path*",
      "has": [{ "type": "host", "value": "landing.designter.hu" }],
      "destination": "/apps/landing-pages/landing-ingyenes-audit/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "(www\\.)?designter.hu" }],
      "destination": "/apps/main-website/:path*"
    }
  ]
}