{
  "builds": [
    {
      "src": "apps/main-website/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/landing-pages/landing-ingyenes-audit/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/ingyenes-audit/assets/(.*)",
      "has": [{ "type": "host", "value": "landing.designter.hu" }],
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/apps/landing-pages/landing-ingyenes-audit/assets/$1"
    },
    {
      "src": "/assets/(.*)",
      "has": [{ "type": "host", "value": "(www\\.)?designter.hu" }],
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/apps/main-website/assets/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/ingyenes-audit/.*",
      "has": [{ "type": "host", "value": "landing.designter.hu" }],
      "dest": "/apps/landing-pages/landing-ingyenes-audit/index.html"
    },
    {
      "src": "/.*",
      "has": [{ "type": "host", "value": "(www\\.)?designter.hu" }],
      "dest": "/apps/main-website/index.html"
    }
  ]
}