{
  "builds": [
    {
      "src": "apps/main-website/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "apps/landing-pages/landing-ingyenes-audit/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(www\\\\.)?designter.hu"
        }
      ],
      "destination": "/apps/main-website/"
    },
    {
      "source": "/ingyenes-audit/(.*)",
      "has": [
        {
          "type": "host",
          "value": "landing.designter.hu"
        }
      ],
      "destination": "/apps/landing-pages/landing-ingyenes-audit/"
    },
    {
      "source": "/ingyenes-audit",
      "has": [
        {
          "type": "host",
          "value": "landing.designter.hu"
        }
      ],
      "destination": "/apps/landing-pages/landing-ingyenes-audit/"
    }
  ]
}
